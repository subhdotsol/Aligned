{
  "expo": {
    "name": "Align",
    "slug": "aligned",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "align",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": { 
      "supportsTablet": true,
      "bundleIdentifier": "com.subhdotsol.aligned"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.subhdotsol.aligned"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f96a6157-d6a8-4c71-b324-4e03b983423f"
      }
    },
    "owner": "subhdotsol"
  }
}
